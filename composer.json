{
	"name": "slub/ddev-liszt-portal",
	"description": "DDEV Container for liszt-portal.de",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"php-http/discovery": true
		},
		"platform": {
			"php": "8.2"
		},
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"repositories": {
		"local-packages": {
			"type": "path",
			"url": "packages/*"
		}
	},
	"require": {
		"ext-intl": "*",
		"b13/doktypemapper": "^1.1",
		"dikastes/zotero-api": "@dev",
		"friendsoftypo3/content-blocks": "^0.7.19",
		"praetorius/vite-asset-collector": "^1.9",
		"slub/liszt-bibliography": "@dev",
		"slub/liszt-common": "@dev",
		"slub/liszt-web": "@dev",
		"typo3/cms-adminpanel": "^12.4",
		"typo3/cms-backend": "^12.0",
		"typo3/cms-belog": "^12.0",
		"typo3/cms-beuser": "^12.0",
		"typo3/cms-core": "^12.0",
		"typo3/cms-dashboard": "^12.0",
		"typo3/cms-extbase": "^12.0",
		"typo3/cms-extensionmanager": "^12.0",
		"typo3/cms-felogin": "^12.0",
		"typo3/cms-filelist": "^12.0",
		"typo3/cms-fluid": "^12.0",
		"typo3/cms-fluid-styled-content": "^12.0",
		"typo3/cms-form": "^12.0",
		"typo3/cms-frontend": "^12.0",
		"typo3/cms-impexp": "^12.0",
		"typo3/cms-info": "^12.0",
		"typo3/cms-install": "^12.0",
		"typo3/cms-lowlevel": "^12.4",
		"typo3/cms-rte-ckeditor": "^12.0",
		"typo3/cms-scheduler": "^12",
		"typo3/cms-seo": "^12.0",
		"typo3/cms-setup": "^12.0",
		"typo3/cms-sys-note": "^12.0",
		"typo3/cms-t3editor": "^12.0",
		"typo3/cms-tstemplate": "^12.0",
		"typo3/cms-viewpage": "^12.0"
	},
	"scripts":{
		"typo3-cms-scripts": [
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	}
}
